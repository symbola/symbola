{
  "private": true,
  "workspaces": [
    "packages/*",
    "configs/*"
  ],
  "description": "Symbol protocols for JavaScript",
  "scripts": {
    "p:immer": "yarn workspace @symbola/immer",
    "p:core": "yarn workspace @symbola/core",
    "p:object": "yarn workspace @symbola/object",
    "p:number": "yarn workspace @symbola/number",
    "p:string": "yarn workspace @symbola/string",
    "c:config": "yarn workspace @symbola/config",
    "c:eslint": "yarn workspace @symbola/config-eslint",
    "c:prettier": "yarn workspace @symbola/config-prettier",
    "c:typescript": "yarn workspace @symbola/config-typescript",
    "c:babel": "yarn workspace @symbola/config-babel",
    "postinstall": "husky install",
    "test": "yarn workspaces foreach -p run test",
    "lint:eslint": "yarn workspaces foreach -p run lint:eslint",
    "lint:prettier": "yarn workspaces foreach -p run lint:prettier",
    "types": "tsc -b"
  },
  "engines": {
    "node": ">= 18"
  },
  "engineStrict": true,
  "volta": {
    "node": "18.13.0"
  },
  "homepage": "https://github.com/symbola/symbola#readme",
  "repository": "github:symbola/symbola",
  "license": "ISC",
  "devDependencies": {
    "@symbola/config": "workspace:^",
    "@types/eslint": "^8",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/prettier": "^2",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@yarnpkg/pnpify": "^4.0.0-rc.36",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^4.2.1",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4"
  },
  "packageManager": "yarn@3.3.1"
}
